{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "ng2-material-import",
  "version": "1.3.7",
  "main": "./index",
  "module": "./material-import.module",
  "typings": "./index.d",
  "author": "“Joseph <“jbriggsr3@gmail.com”> (https://www.jbriggsr3.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rip3rs/ng2-material-import"
  },
  "keywords": [
    "angular2",
    "material",
    "import"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "ng-packagr -p package.json",
    "prepack:tsc": "tsc -p tsconfig.json",
    "prepack:theme": "cp -a src/theme dist"
  },
  "dependencies": {
    "@angular/cdk": "^7.1.1",
    "@angular/material": "^7.1.1",
    "@angular/platform-server": "^7.1.3",
    "hammerjs": "^2.0.8"
  },
  "peerDependencies": {
    "@angular/animations": "^7.1.3",
    "@angular/common": "^7.1.3",
    "@angular/forms": "^7.1.3",
    "@angular/http": "^7.1.3",
    "@angular/platform-browser": "^7.1.3",
    "@angular/platform-browser-dynamic": "^7.1.3"
  },
  "devDependencies": {
    "@angular/compiler": "^7.1.3",
    "@angular/compiler-cli": "^7.1.3",
    "@angular/core": "^7.1.3",
    "ng-packagr": "^4.4.5",
    "rxjs": "^6.2.2",
    "tsickle": "^0.34.0",
    "typescript": ">=3.1.1 <3.2",
    "zone.js": "^0.8.25"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    },
    "whitelistedNonPeerDependencies": [
      "angular",
      "hammerjs"
    ]
  }
}
